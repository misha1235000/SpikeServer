{
  "name": "serverregisterserver",
  "version": "0.0.0",
  "description": "Server side of the register servers project",
  "main": "server.js",
  "scripts": {
    "start": "tsc && node ./scripts/copy_files.js && set NODE_ENV=production&& node -r dotenv/config ./dist/server.js",
    "test-env": "tsc && set NODE_ENV=test",
    "test": "npm run test-env&& mocha --recursive dist/**/*.spec.js --timeout 20000 --exit",
    "copy-files": "node ./scripts/copy_files.js",
    "generate-certs": "node ./scripts/generate_certs.js",
    "validation-test": "npm run test-env&& mocha --recursive 'dist/**/*.validator.spec.js' --exit",
    "integration-test": "npm run test-env&& mocha --recursive 'dist/**/*.router.spec.js' --exit",
    "fuzzy-update": "node ./scripts/db_update_fuzzy.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misha1235000/serverRegisterServer.git"
  },
  "keywords": [
    "serverregister",
    "registerserver"
  ],
  "author": "Michael T & Shaked Manes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/misha1235000/serverRegisterServer/issues"
  },
